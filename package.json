    {
      "name": "Philipp Burgmer"
    }
  ],
  "private": true,
  "scripts": {
    "build": "run-s lib:build demo:build",
    "test": "run-s lib:test",
    "demo:start": "ng serve",
    "demo:build": "ng build --project=demo --configuration production",
    "demo:test": "ng test --project=demo",
    "lib:start": "ng build --project=lib --watch",
    "lib:test": "ng test --project=lib",
    "lib:build": "run-s lib:build:ng lib:build:readme",
    "lib:build:ng": "ng build --project=lib",
    "lib:build:readme": "cpx readme.md dist/lib/",
    "lib:publish:dist": "npm publish dist/lib",
    "lib:publish": "run-s lib:build lib:publish:dist",
    "bump": "bump --commit \"bump version to v\" --no-verify projects/lib/package.json package.json package-lock.json"
  },
  "dependencies": {
    "@angular/animations": "^15.1.2",
    "@angular/common": "^15.1.2",
    "@angular/compiler": "^15.1.2",
    "@angular/core": "^15.1.2",
    "@angular/forms": "^15.1.2",
    "@angular/platform-browser": "^15.1.2",
    "@angular/platform-browser-dynamic": "^15.1.2",
    "@angular/router": "^15.1.2",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.3",
    "@angular/cli": "^15.1.3",
    "@angular/compiler-cli": "^15.1.2",
    "@angular/language-service": "^15.1.2",
    "@larscom/cpx": "^2.0.1",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.1.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.4",
    "version-bump-prompt": "^5.0.6"
  }
}
